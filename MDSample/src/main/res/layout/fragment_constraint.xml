<?xml version="1.0" encoding="utf-8"?>

<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="24dp"
    android:id="@+id/constraintLayout">


    <ImageView
        android:src="@color/accent_material_light"
        android:layout_width="0dp"
        android:layout_height="200dp"
        android:id="@+id/imageView2"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/constraintLayout"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout"
        tools:layout_constraintRight_creator="1" />

    <android.support.design.widget.FloatingActionButton
        android:src="@drawable/ic_menu_send"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        app:fabSize="normal"
        tools:layout_editor_absoluteX="323dp"
        tools:layout_editor_absoluteY="172dp"
        android:id="@+id/floatingActionButton3"
        app:rippleColor="@color/red"
        app:elevation="8dp"
        app:backgroundTint="@color/red"
        android:tint="@color/bright_foreground_inverse_material_light"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toRightOf="@+id/constraintLayout"
        android:layout_marginRight="32dp"
        android:layout_marginEnd="32dp"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        tools:layout_constraintBottom_creator="1" />

    <ImageView
        android:src="@drawable/abc_btn_radio_material"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="275dp"
        android:id="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:layout_marginTop="75dp"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintRight_toLeftOf="@+id/textView3"
        android:layout_marginRight="56dp"
        android:layout_marginEnd="56dp"
        tools:layout_constraintRight_creator="0" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="275dp"
        android:id="@+id/textView3"
        app:layout_constraintLeft_toLeftOf="@+id/constraintLayout"
        android:layout_marginLeft="121dp"
        android:layout_marginStart="121dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        tools:layout_constraintTop_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        tools:layout_constraintBottom_creator="0"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="299dp"
        android:id="@+id/textView5"
        app:layout_constraintLeft_toLeftOf="@+id/textView3"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:layout_constraintTop_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        tools:layout_constraintBottom_creator="1" />

    <EditText
        android:layout_width="194dp"
        android:layout_height="43dp"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="331dp"
        android:id="@+id/editText"
        style="@style/Base.Widget.AppCompat.EditText"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        tools:layout_constraintLeft_creator="0"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        android:layout_marginTop="16dp"
        tools:layout_constraintTop_creator="0" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="316dp"
        tools:layout_editor_absoluteY="580dp"
        android:id="@+id/button2"
        app:layout_constraintLeft_toRightOf="@+id/button3"
        android:layout_marginLeft="17dp"
        android:layout_marginStart="17dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/constraintLayout"
        android:layout_marginBottom="30dp"
        tools:layout_constraintBottom_creator="1" />

    <Button
        android:text="Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="580dp"
        android:id="@+id/button3"
        app:layout_constraintRight_toLeftOf="@+id/floatingActionButton3"
        android:layout_marginRight="24dp"
        android:layout_marginEnd="24dp"
        tools:layout_constraintRight_creator="0"
        app:layout_constraintBottom_toBottomOf="@+id/button2"
        tools:layout_constraintBottom_creator="0" />

</android.support.constraint.ConstraintLayout>